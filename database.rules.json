{
    "rules": {
        "stocks": {
            ".read": true,
            ".write": false
        },
        "system": {
            ".read": true,
            ".write": false
        },
        "user_activities": {
            "$uid": {
                ".read": "auth != null && auth.uid == $uid",
                ".write": "auth != null && auth.uid == $uid"
            }
        },
        "ai_requests": {
            "$uid": {
                ".read": true,
                ".write": "auth != null && auth.uid == $uid"
            }
        },
        "search_requests": {
            "$uid": {
                ".read": "auth != null && auth.uid == $uid",
                ".write": "auth != null && auth.uid == $uid"
            }
        },
        "search_results": {
            "$uid": {
                ".read": "auth != null && auth.uid == $uid",
                ".write": false
            }
        },
        "history_requests": {
            "$symbol": {
                ".read": true,
                ".write": true
            }
        },
        "custom_symbols": {
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "users": {
            ".read": true,
            "$uid": {
                "comment": {
                    ".write": "auth != null && auth.uid == $uid"
                }
            }
        }
    }
}